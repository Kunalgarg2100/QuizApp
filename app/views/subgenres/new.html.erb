<% provide(:title, 'Add Subgenre') %>
<h1>Add Subgenre</h1>
<div class="row">
  <div class="col-md-4 col-md-offset-4">
     <%= form_for @subgenre do |f| %>
     <div class="field">
      <%= f.label :Name %>
	    <%= f.text_field :cont, class: 'form-control' %>
    </div>	
    <%= hidden_field_tag :genre_id, $hello %>

      <%= f.submit 'Create Subgenre' , class: 'btn btn-info'%>
    <% end %>
    </div>
    </div>

